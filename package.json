{
  "name": "zss-utils",
  "version": "0.2.3",
  "description": "Zero-runtime StyleSheet Utilities",
  "keywords": [
    "zero-runtime",
    "stylesheet",
    "utilities",
    "css-in-js"
  ],
  "author": "Refirst",
  "repository": "github:zss-in-js/zss-utils",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsdown"
  },
  "devDependencies": {
    "tsdown": "^0.11.12",
    "typescript": "^5.8.2"
  }
}
